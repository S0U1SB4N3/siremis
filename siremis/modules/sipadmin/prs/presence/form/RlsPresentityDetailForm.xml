<?xml version="1.0" encoding="UTF-8"?>
<EasyForm Name="RlsPresentityDetailForm" Class="EasyForm" FormType="" jsClass="jbForm" Title="RLS Presentity Detail" Description="" BizDataObj="sipadmin.prs.presence.do.RlsPresentityDO" TemplateEngine="Smarty" TemplateFile="detail_elementset.tpl" EventName="" MessageFile="">
    <DataPanel>
		<Element Name="fld_Id" ElementSet="General" Hidden="Y" Class="LabelText" FieldName="Id" Label="Id" AllowURLParam="Y"/>
		<Element Name="fld_rlsubs_did" ElementSet="General" Class="LabelText" FieldName="rlsubs_did" Label="Rlsubs did" AllowURLParam="N"/>
       	<Element Name="fld_resource_uri" ElementSet="General" Class="LabelText" FieldName="resource_uri" Label="Resource URI" AllowURLParam="N"/>
       	<Element Name="fld_content_type" ElementSet="General" Class="LabelText" FieldName="content_type" Label="Content Type" AllowURLParam="N"/>
		<Element Name="fld_presence_state_detail" ElementSet="General" Class="LabelText" FieldName="presence_state_detail" Label="Presence State" AllowURLParam="N" />
       	<Element Name="fld_expires" ElementSet="General" Class="LabelText" FieldName="expires" Label="Expires" AllowURLParam="N"/>
       	<Element Name="fld_updated" ElementSet="General" Class="LabelText" FieldName="updated" Label="Updated" AllowURLParam="N"/>
       	<Element Name="fld_auth_state" ElementSet="General" Class="LabelText" FieldName="auth_state" Label="Auth State" AllowURLParam="N"/>
       	<Element Name="fld_reason" ElementSet="General" Class="LabelText" FieldName="reason" Label="Reason" AllowURLParam="N"/>
    </DataPanel>
    <ActionPanel>       
        <Element Name="btn_new" Class="Button" Text="Add" CssClass="button_gray_add" Description="new record (Insert)" Hidden="Y">
			<EventHandler Name="btn_new_onclick" Event="onclick" Function="SwitchForm(sipadmin.prs.presence.form.RlsPresentityNewForm)"  ShortcutKey="Insert" ContextMenu="New" />
        </Element>          
        <Element Name="btn_edit" Class="Button" Text="Edit" CssClass="button_gray_m" Description="edit record (Ctrl+E)" Hidden="Y">
			<EventHandler Name="btn_new_onclick" Event="onclick" Function="SwitchForm(sipadmin.prs.presence.form.RlsPresentityEditForm,{@:Elem[fld_Id].Value})"  ShortcutKey="Ctrl+E" ContextMenu="Edit" />
        </Element>
		<Element Name="btn_copy" Class="Button" Text="Copy" CssClass="button_gray_m" Description="copy record (Ctrl+C)" Hidden="Y">
            <EventHandler Name="onclick" Event="onclick" EventLogMsg="" Function="CopyRecord({@:Elem[fld_Id].Value})" RedirectPage="form=sipadmin.prs.presence.form.RlsPresentityCopyForm&amp;fld:Id={@:Elem[fld_Id].Value}" ShortcutKey="Ctrl+C" ContextMenu="Copy"/>
        </Element> 
        <Element Name="btn_delete" Class="Button" Text="Delete" CssClass="button_gray_m" Description="delete record (Delete)">
            <EventHandler Name="del_onclick" Event="onclick" EventLogMsg="" Function="DeleteRecord({@:Elem[fld_Id].Value})"  RedirectPage="form=sipadmin.prs.presence.form.RlsPresentityListForm" ShortcutKey="Ctrl+Delete" ContextMenu="Delete" />
        </Element>
        <Element Name="btn_cancel" Class="Button" Text="Back" CssClass="button_gray_m">
            <EventHandler Name="btn_cancel_onclick" Event="onclick" Function="SwitchForm(sipadmin.prs.presence.form.RlsPresentityListForm)"  ShortcutKey="Escape" ContextMenu="Cancel" />
        </Element>         
    </ActionPanel> 
    <NavPanel>
    </NavPanel> 
    <SearchPanel>
    </SearchPanel>
</EasyForm>
