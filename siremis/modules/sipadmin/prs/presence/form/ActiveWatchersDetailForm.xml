<?xml version="1.0" encoding="UTF-8"?>
<EasyForm Name="ActiveWatchersDetailForm" Class="EasyForm" FormType="" jsClass="jbForm" Title="Active Watchers Detail" Description="" BizDataObj="sipadmin.prs.presence.do.ActiveWatchersDO" TemplateEngine="Smarty" TemplateFile="detail_elementset.tpl" EventName="" MessageFile="">
    <DataPanel>
		<Element Name="fld_Id" ElementSet="General" Hidden="Y" Class="LabelText" FieldName="Id" Label="Id" AllowURLParam="Y"/>
