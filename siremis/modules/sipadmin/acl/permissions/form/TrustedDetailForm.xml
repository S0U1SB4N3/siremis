<?xml version="1.0" encoding="UTF-8"?>
<EasyForm Name="TrustedDetailForm" Class="EasyForm" FormType="" jsClass="jbForm" Title="Trusted Detail" Description="" BizDataObj="sipadmin.acl.permissions.do.TrustedDO" TemplateEngine="Smarty" TemplateFile="detail_elementset.tpl" EventName="" MessageFile="">
    <DataPanel>
		<Element Name="fld_Id" ElementSet="General" Hidden="Y" Class="LabelText" FieldName="Id" Label="Id" AllowURLParam="Y"/>
       	<Element Name="fld_src_ip" ElementSet="General" Class="LabelText" FieldName="src_ip" Label="Src Ip" AllowURLParam="N"/>
       	<Element Name="fld_proto" ElementSet="General" Class="LabelText" FieldName="proto" Label="Proto" AllowURLParam="N"/>
       	<Element Name="fld_from_pattern" ElementSet="General" Class="LabelText" FieldName="from_pattern" Label="From Pattern" AllowURLParam="N"/>
       	<Element Name="fld_tag" ElementSet="General" Class="LabelText" FieldName="tag" Label="Tag" AllowURLParam="N"/>
    </DataPanel>
    <ActionPanel>       
        <Element Name="btn_new" Class="Button" Text="Add" CssClass="button_gray_add" Description="new record (Insert)">
			<EventHandler Name="btn_new_onclick" Event="onclick" Function="SwitchForm(sipadmin.acl.permissions.form.TrustedNewForm)"  ShortcutKey="Insert" ContextMenu="New" />
        </Element>          
        <Element Name="btn_edit" Class="Button" Text="Edit" CssClass="button_gray_m" Description="edit record (Ctrl+E)">
			<EventHandler Name="btn_new_onclick" Event="onclick" Function="SwitchForm(sipadmin.acl.permissions.form.TrustedEditForm,{@:Elem[fld_Id].Value})"  ShortcutKey="Ctrl+E" ContextMenu="Edit" />
        </Element>
		<Element Name="btn_copy" Class="Button" Text="Copy" CssClass="button_gray_m" Description="copy record (Ctrl+C)">
            <EventHandler Name="onclick" Event="onclick" EventLogMsg="" Function="CopyRecord({@:Elem[fld_Id].Value})" RedirectPage="form=sipadmin.acl.permissions.form.TrustedCopyForm&amp;fld:Id={@:Elem[fld_Id].Value}" ShortcutKey="Ctrl+C" ContextMenu="Copy"/>
        </Element> 
        <Element Name="btn_delete" Class="Button" Text="Delete" CssClass="button_gray_m" Description="delete record (Delete)">
            <EventHandler Name="del_onclick" Event="onclick" EventLogMsg="" Function="DeleteRecord({@:Elem[fld_Id].Value})"  RedirectPage="form=sipadmin.acl.permissions.form.TrustedListForm" ShortcutKey="Ctrl+Delete" ContextMenu="Delete" />
        </Element>
        <Element Name="btn_cancel" Class="Button" Text="Back" CssClass="button_gray_m">
            <EventHandler Name="btn_cancel_onclick" Event="onclick" Function="SwitchForm(sipadmin.acl.permissions.form.TrustedListForm)"  ShortcutKey="Escape" ContextMenu="Cancel" />
        </Element>         
    </ActionPanel> 
    <NavPanel>
    </NavPanel> 
    <SearchPanel>
    </SearchPanel>
</EasyForm>
