<?xml version="1.0" encoding="UTF-8"?>
<EasyForm Name="LocationAttrsDetailForm" Class="EasyForm" FormType="" jsClass="jbForm" Title="Location Attrs Detail" Description="" BizDataObj="sipadmin.sbs.usrloc.do.LocationAttrsDO" TemplateEngine="Smarty" TemplateFile="detail_elementset.tpl" EventName="" MessageFile="">
    <DataPanel>
		<Element Name="fld_Id" ElementSet="General" Hidden="Y" Class="LabelText" FieldName="Id" Label="Id" AllowURLParam="Y"/>
       	<Element Name="fld_ruid" ElementSet="General" Class="LabelText" FieldName="ruid" Label="RUID" AllowURLParam="N"/>
       	<Element Name="fld_username" ElementSet="General" Class="LabelText" FieldName="username" Label="Username" AllowURLParam="N"/>
       	<Element Name="fld_domain" ElementSet="General" Class="LabelText" FieldName="domain" Label="Domain" AllowURLParam="N"/>
       	<Element Name="fld_aname" ElementSet="General" Class="LabelText" FieldName="aname" Label="Attr Nname" AllowURLParam="N"/>
       	<Element Name="fld_atype" ElementSet="General" Class="LabelText" FieldName="atype" Label="Attr Type" AllowURLParam="N"/>
       	<Element Name="fld_avalue" ElementSet="General" Class="LabelText" FieldName="avalue" Label="Attr Value" AllowURLParam="N"/>
       	<Element Name="fld_last_modified" ElementSet="General" Class="LabelText" FieldName="last_modified" Label="Last Modified" AllowURLParam="N"/>
    </DataPanel>
    <ActionPanel>       
        <Element Name="btn_new" Class="Button" Text="Add" CssClass="button_gray_add" Description="new record (Insert)">
			<EventHandler Name="btn_new_onclick" Event="onclick" Function="SwitchForm(sipadmin.sbs.usrloc.form.LocationAttrsNewForm)"  ShortcutKey="Insert" ContextMenu="New" />
        </Element>          
        <Element Name="btn_edit" Class="Button" Text="Edit" CssClass="button_gray_m" Description="edit record (Ctrl+E)">
			<EventHandler Name="btn_new_onclick" Event="onclick" Function="SwitchForm(sipadmin.sbs.usrloc.form.LocationAttrsEditForm,{@:Elem[fld_Id].Value})"  ShortcutKey="Ctrl+E" ContextMenu="Edit" />
        </Element>
		<Element Name="btn_copy" Class="Button" Text="Copy" CssClass="button_gray_m" Description="copy record (Ctrl+C)">
            <EventHandler Name="onclick" Event="onclick" EventLogMsg="" Function="CopyRecord({@:Elem[fld_Id].Value})" RedirectPage="form=sipadmin.sbs.usrloc.form.LocationAttrsCopyForm&amp;fld:Id={@:Elem[fld_Id].Value}" ShortcutKey="Ctrl+C" ContextMenu="Copy"/>
        </Element> 
        <Element Name="btn_delete" Class="Button" Text="Delete" CssClass="button_gray_m" Description="delete record (Delete)">
            <EventHandler Name="del_onclick" Event="onclick" EventLogMsg="" Function="DeleteRecord({@:Elem[fld_Id].Value})"  RedirectPage="form=sipadmin.sbs.usrloc.form.LocationAttrsListForm" ShortcutKey="Ctrl+Delete" ContextMenu="Delete" />
        </Element>
        <Element Name="btn_cancel" Class="Button" Text="Back" CssClass="button_gray_m">
            <EventHandler Name="btn_cancel_onclick" Event="onclick" Function="SwitchForm(sipadmin.sbs.usrloc.form.LocationAttrsListForm)"  ShortcutKey="Escape" ContextMenu="Cancel" />
        </Element>         
    </ActionPanel> 
    <NavPanel>
    </NavPanel> 
    <SearchPanel>
    </SearchPanel>
</EasyForm>
