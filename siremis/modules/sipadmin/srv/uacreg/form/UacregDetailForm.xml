<?xml version="1.0" encoding="UTF-8"?>
<EasyForm Name="UacregDetailForm" Class="EasyForm" FormType="" jsClass="jbForm" Title="Uacreg Detail" Description="" BizDataObj="sipadmin.srv.uacreg.do.UacregDO" TemplateEngine="Smarty" TemplateFile="detail_elementset.tpl" EventName="" MessageFile="">
    <DataPanel>
		<Element Name="fld_Id" ElementSet="General" Hidden="Y" Class="LabelText" FieldName="Id" Label="Id" AllowURLParam="Y"/>
       	<Element Name="fld_l_uuid" ElementSet="General" Class="LabelText" FieldName="l_uuid" Label="L Uuid" AllowURLParam="N"/>
       	<Element Name="fld_l_username" ElementSet="General" Class="LabelText" FieldName="l_username" Label="L Username" AllowURLParam="N"/>
       	<Element Name="fld_l_domain" ElementSet="General" Class="LabelText" FieldName="l_domain" Label="L Domain" AllowURLParam="N"/>
       	<Element Name="fld_r_username" ElementSet="General" Class="LabelText" FieldName="r_username" Label="R Username" AllowURLParam="N"/>
       	<Element Name="fld_r_domain" ElementSet="General" Class="LabelText" FieldName="r_domain" Label="R Domain" AllowURLParam="N"/>
       	<Element Name="fld_realm" ElementSet="General" Class="LabelText" FieldName="realm" Label="Realm" AllowURLParam="N"/>
       	<Element Name="fld_auth_username" ElementSet="General" Class="LabelText" FieldName="auth_username" Label="Auth Username" AllowURLParam="N"/>
       	<Element Name="fld_auth_password" ElementSet="General" Class="LabelText" FieldName="auth_password" Label="Auth Password" AllowURLParam="N"/>
       	<Element Name="fld_auth_proxy" ElementSet="General" Class="LabelText" FieldName="auth_proxy" Label="Auth Proxy" AllowURLParam="N"/>
       	<Element Name="fld_expires" ElementSet="General" Class="LabelText" FieldName="expires" Label="Expires" AllowURLParam="N"/>
    </DataPanel>
    <ActionPanel>       
        <Element Name="btn_new" Class="Button" Text="Add" CssClass="button_gray_add" Description="new record (Insert)">
			<EventHandler Name="btn_new_onclick" Event="onclick" Function="SwitchForm(sipadmin.srv.uacreg.form.UacregNewForm)"  ShortcutKey="Insert" ContextMenu="New" />
        </Element>          
        <Element Name="btn_edit" Class="Button" Text="Edit" CssClass="button_gray_m" Description="edit record (Ctrl+E)">
			<EventHandler Name="btn_new_onclick" Event="onclick" Function="SwitchForm(sipadmin.srv.uacreg.form.UacregEditForm,{@:Elem[fld_Id].Value})"  ShortcutKey="Ctrl+E" ContextMenu="Edit" />
        </Element>
		<Element Name="btn_copy" Class="Button" Text="Copy" CssClass="button_gray_m" Description="copy record (Ctrl+C)">
            <EventHandler Name="onclick" Event="onclick" EventLogMsg="" Function="CopyRecord({@:Elem[fld_Id].Value})" RedirectPage="form=sipadmin.srv.uacreg.form.UacregCopyForm&amp;fld:Id={@:Elem[fld_Id].Value}" ShortcutKey="Ctrl+C" ContextMenu="Copy"/>
        </Element> 
        <Element Name="btn_delete" Class="Button" Text="Delete" CssClass="button_gray_m" Description="delete record (Delete)">
            <EventHandler Name="del_onclick" Event="onclick" EventLogMsg="" Function="DeleteRecord({@:Elem[fld_Id].Value})"  RedirectPage="form=sipadmin.srv.uacreg.form.UacregListForm" ShortcutKey="Ctrl+Delete" ContextMenu="Delete" />
        </Element>
        <Element Name="btn_cancel" Class="Button" Text="Back" CssClass="button_gray_m">
            <EventHandler Name="btn_cancel_onclick" Event="onclick" Function="SwitchForm(sipadmin.srv.uacreg.form.UacregListForm)"  ShortcutKey="Escape" ContextMenu="Cancel" />
        </Element>         
    </ActionPanel> 
    <NavPanel>
    </NavPanel> 
    <SearchPanel>
    </SearchPanel>
</EasyForm>
