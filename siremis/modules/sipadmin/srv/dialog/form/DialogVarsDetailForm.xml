<?xml version="1.0" encoding="UTF-8"?>
<EasyForm Name="DialogVarsDetailForm" Class="EasyForm" FormType="" jsClass="jbForm" Title="Dialog Vars Detail" Description="" BizDataObj="sipadmin.srv.dialog.do.DialogVarsDO" TemplateEngine="Smarty" TemplateFile="detail_elementset.tpl" EventName="" MessageFile="">
    <DataPanel>
		<Element Name="fld_Id" ElementSet="General" Hidden="Y" Class="LabelText" FieldName="Id" Label="Id" AllowURLParam="Y"/>
       	<Element Name="fld_hash_entry" ElementSet="General" Class="LabelText" FieldName="hash_entry" Label="Hash Entry" AllowURLParam="N"/>
       	<Element Name="fld_hash_id" ElementSet="General" Class="LabelText" FieldName="hash_id" Label="Hash Id" AllowURLParam="N"/>
       	<Element Name="fld_dialog_key" ElementSet="General" Class="LabelText" FieldName="dialog_key" Label="Dialog Key" AllowURLParam="N"/>
       	<Element Name="fld_dialog_value" ElementSet="General" Class="LabelText" FieldName="dialog_value" Label="Dialog Value" AllowURLParam="N"/>
    </DataPanel>
    <ActionPanel>       
        <Element Name="btn_new" Class="Button" Text="Add" CssClass="button_gray_add" Description="new record (Insert)">
			<EventHandler Name="btn_new_onclick" Event="onclick" Function="SwitchForm(sipadmin.srv.dialog.form.DialogVarsNewForm)"  ShortcutKey="Insert" ContextMenu="New" />
        </Element>          
        <Element Name="btn_edit" Class="Button" Text="Edit" CssClass="button_gray_m" Description="edit record (Ctrl+E)">
			<EventHandler Name="btn_new_onclick" Event="onclick" Function="SwitchForm(sipadmin.srv.dialog.form.DialogVarsEditForm,{@:Elem[fld_Id].Value})"  ShortcutKey="Ctrl+E" ContextMenu="Edit" />
        </Element>
		<Element Name="btn_copy" Class="Button" Text="Copy" CssClass="button_gray_m" Description="copy record (Ctrl+C)">
            <EventHandler Name="onclick" Event="onclick" EventLogMsg="" Function="CopyRecord({@:Elem[fld_Id].Value})" RedirectPage="form=sipadmin.srv.dialog.form.DialogVarsCopyForm&amp;fld:Id={@:Elem[fld_Id].Value}" ShortcutKey="Ctrl+C" ContextMenu="Copy"/>
        </Element> 
        <Element Name="btn_delete" Class="Button" Text="Delete" CssClass="button_gray_m" Description="delete record (Delete)">
            <EventHandler Name="del_onclick" Event="onclick" EventLogMsg="" Function="DeleteRecord({@:Elem[fld_Id].Value})"  RedirectPage="form=sipadmin.srv.dialog.form.DialogVarsListForm" ShortcutKey="Ctrl+Delete" ContextMenu="Delete" />
        </Element>
        <Element Name="btn_cancel" Class="Button" Text="Back" CssClass="button_gray_m">
            <EventHandler Name="btn_cancel_onclick" Event="onclick" Function="SwitchForm(sipadmin.srv.dialog.form.DialogVarsListForm)"  ShortcutKey="Escape" ContextMenu="Cancel" />
        </Element>         
    </ActionPanel> 
    <NavPanel>
    </NavPanel> 
    <SearchPanel>
    </SearchPanel>
</EasyForm>
