<?xml version="1.0" encoding="UTF-8"?>
<EasyForm Name="MohqcallsDetailForm" Class="EasyForm" FormType="" jsClass="jbForm" Title="Mohqcalls Detail" Description="" BizDataObj="sipadmin.msc.mohqcalls.do.MohqcallsDO" TemplateEngine="Smarty" TemplateFile="detail_elementset.tpl" EventName="" MessageFile="">
