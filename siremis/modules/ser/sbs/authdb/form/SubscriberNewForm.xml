<?xml version="1.0" encoding="UTF-8"?>
<EasyForm Name="SubscriberNewForm" Class="EasyForm" FormType="New" jsClass="jbForm" Title="New Subscriber" Description="" BizDataObj="sipadmin.sbs.authdb.do.SubscriberDO" TemplateEngine="Smarty" TemplateFile="detail.tpl" EventName="" MessageFile="">
