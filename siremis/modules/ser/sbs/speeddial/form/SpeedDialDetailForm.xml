<?xml version="1.0" encoding="UTF-8"?>
<EasyForm Name="SpeedDialDetailForm" Class="EasyForm" FormType="" jsClass="jbForm" Title="Speed Dial Detail" Description="" BizDataObj="sipadmin.sbs.speeddial.do.SpeedDialDO" TemplateEngine="Smarty" TemplateFile="detail_elementset.tpl" EventName="" MessageFile="">
    <DataPanel>
		<Element Name="fld_Id" ElementSet="General" Hidden="Y" Class="LabelText" FieldName="Id" Label="Id" AllowURLParam="Y"/>
       	<Element Name="fld_username" ElementSet="General" Class="LabelText" FieldName="username" Label="Username" AllowURLParam="N"/>
       	<Element Name="fld_domain" ElementSet="General" Class="LabelText" FieldName="domain" Label="Domain" AllowURLParam="N"/>
       	<Element Name="fld_sd_username" ElementSet="General" Class="LabelText" FieldName="sd_username" Label="Sd Username" AllowURLParam="N"/>
       	<Element Name="fld_sd_domain" ElementSet="General" Class="LabelText" FieldName="sd_domain" Label="Sd Domain" AllowURLParam="N"/>
       	<Element Name="fld_new_uri" ElementSet="General" Class="LabelText" FieldName="new_uri" Label="New Uri" AllowURLParam="N"/>
       	<Element Name="fld_fname" ElementSet="General" Class="LabelText" FieldName="fname" Label="Fname" AllowURLParam="N"/>
       	<Element Name="fld_lname" ElementSet="General" Class="LabelText" FieldName="lname" Label="Lname" AllowURLParam="N"/>
       	<Element Name="fld_description" ElementSet="General" Class="LabelText" FieldName="description" Label="Description" AllowURLParam="N"/>
    </DataPanel>
    <ActionPanel>       
        <Element Name="btn_new" Class="Button" Text="Add" CssClass="button_gray_add" Description="new record (Insert)">
			<EventHandler Name="btn_new_onclick" Event="onclick" Function="SwitchForm(sipadmin.sbs.speeddial.form.SpeedDialNewForm)"  ShortcutKey="Insert" ContextMenu="New" />
        </Element>          
        <Element Name="btn_edit" Class="Button" Text="Edit" CssClass="button_gray_m" Description="edit record (Ctrl+E)">
			<EventHandler Name="btn_new_onclick" Event="onclick" Function="SwitchForm(sipadmin.sbs.speeddial.form.SpeedDialEditForm,{@:Elem[fld_Id].Value})"  ShortcutKey="Ctrl+E" ContextMenu="Edit" />
        </Element>
		<Element Name="btn_copy" Class="Button" Text="Copy" CssClass="button_gray_m" Description="copy record (Ctrl+C)">
            <EventHandler Name="onclick" Event="onclick" EventLogMsg="" Function="CopyRecord({@:Elem[fld_Id].Value})" RedirectPage="form=sipadmin.sbs.speeddial.form.SpeedDialCopyForm&amp;fld:Id={@:Elem[fld_Id].Value}" ShortcutKey="Ctrl+C" ContextMenu="Copy"/>
        </Element> 
        <Element Name="btn_delete" Class="Button" Text="Delete" CssClass="button_gray_m" Description="delete record (Delete)">
            <EventHandler Name="del_onclick" Event="onclick" EventLogMsg="" Function="DeleteRecord({@:Elem[fld_Id].Value})"  RedirectPage="form=sipadmin.sbs.speeddial.form.SpeedDialListForm" ShortcutKey="Ctrl+Delete" ContextMenu="Delete" />
        </Element>
        <Element Name="btn_cancel" Class="Button" Text="Back" CssClass="button_gray_m">
            <EventHandler Name="btn_cancel_onclick" Event="onclick" Function="SwitchForm(sipadmin.sbs.speeddial.form.SpeedDialListForm)"  ShortcutKey="Escape" ContextMenu="Cancel" />
        </Element>         
    </ActionPanel> 
    <NavPanel>
    </NavPanel> 
    <SearchPanel>
    </SearchPanel>
</EasyForm>
