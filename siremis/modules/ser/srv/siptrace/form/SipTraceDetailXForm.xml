<?xml version="1.0" encoding="UTF-8"?>
<EasyForm Name="SipTraceDetailXForm" Class="sipadmin.common.form.EasyDetailXForm" Title="Sip Trace Record Detail" jsClass="jbForm" BizDataObj="sipadmin.srv.siptrace.do.SipTraceDetailXDO" TemplateEngine="Smarty" TemplateFile="detail_elementset.tpl">
    <DataPanel>
		<Element Name="fld_Id" ElementSet="General" Hidden="Y" Class="LabelText" FieldName="Id" Label="Id" AllowURLParam="Y"/>
       	<Element Name="fld_method" ElementSet="General" Class="LabelText" FieldName="method" Label="Method" AllowURLParam="N"/>
       	<Element Name="fld_status" ElementSet="General" Class="LabelText" FieldName="status" Label="Status" AllowURLParam="N"/>
       	<Element Name="fld_callid" ElementSet="General" Class="LabelText" FieldName="callid" Label="Callid" AllowURLParam="N"/>
		<Element Name="fld_msg_detail" ElementSet="General" Class="CKEditor"  mode="notoolbar"  Config="resize_maxWidth:640,resize_minWidth:640,resize_minHeight:300" width="640" height="300" FieldName="msg_detail" Label="Msg"  Sortable="Y" AllowURLParam="N" Translatable="N" OnEventLog="N"  />
       	<Element Name="fld_traced_user" ElementSet="General" Class="LabelText" FieldName="traced_user" Label="Traced User" AllowURLParam="N"/>
       	<Element Name="fld_fromip" ElementSet="General" Class="LabelText" FieldName="fromip" Label="Fromip" AllowURLParam="N"/>
       	<Element Name="fld_toip" ElementSet="General" Class="LabelText" FieldName="toip" Label="Toip" AllowURLParam="N"/>
       	<Element Name="fld_fromtag" ElementSet="General" Class="LabelText" FieldName="fromtag" Label="Fromtag" AllowURLParam="N"/>
       	<Element Name="fld_time_stamp" ElementSet="General" Class="LabelText" FieldName="time_stamp" Label="Time Stamp" AllowURLParam="N"/>
	<Element Name="fld_time_us" ElementSet="General" Class="LabelText" FieldName="time_us" Label="Time us" AllowURLParam="N"/>
       	<Element Name="fld_direction" ElementSet="General" Class="LabelText" FieldName="direction" Label="Direction" AllowURLParam="N"/>
    </DataPanel>
    <ActionPanel>       
        <Element Name="btn_new" Class="Button" Text="Add" CssClass="button_gray_add" Description="new record (Insert)">
			<EventHandler Name="btn_new_onclick" Event="onclick" Function="SwitchForm(sipadmin.srv.siptrace.form.SipTraceNewForm)" ShortcutKey="Insert" ContextMenu="New"/>
        </Element>          
        <Element Name="btn_edit" Class="Button" Text="Edit" CssClass="button_gray_m" Description="edit record (Ctrl+E)">
			<EventHandler Name="btn_new_onclick" Event="onclick" Function="SwitchForm(sipadmin.srv.siptrace.form.SipTraceEditForm,{@:Elem[fld_Id].Value})" ShortcutKey="Ctrl+E" ContextMenu="Edit"/>
        </Element>
		<Element Name="btn_copy" Class="Button" Text="Copy" CssClass="button_gray_m" Description="copy record (Ctrl+C)">
            <EventHandler Name="onclick" Event="onclick" EventLogMsg="" Function="CopyRecord({@:Elem[fld_Id].Value})" RedirectPage="form=sipadmin.srv.siptrace.form.SipTraceCopyForm&amp;fld:Id={@:Elem[fld_Id].Value}" ShortcutKey="Ctrl+C" ContextMenu="Copy"/>
        </Element> 
        <Element Name="btn_delete" Class="Button" Text="Delete" CssClass="button_gray_m" Description="delete record (Delete)">
            <EventHandler Name="del_onclick" Event="onclick" EventLogMsg="" Function="DeleteRecord({@:Elem[fld_Id].Value})" RedirectPage="form=sipadmin.srv.siptrace.form.SipTraceListForm" ShortcutKey="Ctrl+Delete" ContextMenu="Delete"/>
        </Element>
        <Element Name="btn_cancel" Class="Button" Text="Back" CssClass="button_gray_m">
            <EventHandler Name="btn_cancel_onclick" Event="onclick" Function="SwitchForm(sipadmin.srv.siptrace.form.SipTraceListForm)" ShortcutKey="Escape" ContextMenu="Cancel"/>
        </Element>         
    </ActionPanel> 
    <NavPanel>
    </NavPanel> 
    <SearchPanel>
    </SearchPanel>
</EasyForm>
