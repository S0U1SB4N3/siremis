<?xml version="1.0" encoding="UTF-8"?>
<EasyForm Name="DialplanCopyForm" Class="EasyForm" FormType="Edit" jsClass="jbForm" Title="Copy Dialplan" Description="" BizDataObj="ser.srv.dialplan.do.DialplanDO" TemplateEngine="Smarty" TemplateFile="detail.tpl" EventName="" MessageFile="">
    <DataPanel>
        <Element Name="fld_Id" Class="Hidden" FieldName="Id" Label="Id" AllowURLParam="Y"/>	
		<Element Name="fld_dpid" Class="InputText" FieldName="dpid" Label="Dpid"  AllowURLParam="N" CssClass="input_text" CssErrorClass="input_text_error" CssFocusClass="input_text_focus"/>
		<Element Name="fld_pr" Class="InputText" FieldName="pr" Label="Pr"  AllowURLParam="N" CssClass="input_text" CssErrorClass="input_text_error" CssFocusClass="input_text_focus"/>
		<Element Name="fld_match_op" Class="InputText" FieldName="match_op" Label="Match Op"  AllowURLParam="N" CssClass="input_text" CssErrorClass="input_text_error" CssFocusClass="input_text_focus"/>
		<Element Name="fld_match_exp" Class="InputText" FieldName="match_exp" Label="Match Exp"  AllowURLParam="N" CssClass="input_text" CssErrorClass="input_text_error" CssFocusClass="input_text_focus"/>
		<Element Name="fld_match_len" Class="InputText" FieldName="match_len" Label="Match Len"  AllowURLParam="N" CssClass="input_text" CssErrorClass="input_text_error" CssFocusClass="input_text_focus"/>
		<Element Name="fld_subst_exp" Class="InputText" FieldName="subst_exp" Label="Subst Exp"  AllowURLParam="N" CssClass="input_text" CssErrorClass="input_text_error" CssFocusClass="input_text_focus"/>
		<Element Name="fld_repl_exp" Class="InputText" FieldName="repl_exp" Label="Repl Exp"  AllowURLParam="N" CssClass="input_text" CssErrorClass="input_text_error" CssFocusClass="input_text_focus"/>
		<Element Name="fld_attrs" Class="InputText" FieldName="attrs" Label="Attrs"  AllowURLParam="N" CssClass="input_text" CssErrorClass="input_text_error" CssFocusClass="input_text_focus"/>
    </DataPanel>
    <ActionPanel>
        <Element Name="btn_save" Class="Button" Text="Save" CssClass="button_gray_m">
            <EventHandler Name="save_onclick" EventLogMsg="" Event="onclick" Function="InsertRecord()" RedirectPage="form=ser.srv.dialplan.form.DialplanDetailForm&amp;fld:Id={@ser.srv.dialplan.do.DialplanDO:Field[Id].Value}"  ShortcutKey="Ctrl+Enter" ContextMenu="Save" />
        </Element>
        <Element Name="btn_cancel" Class="Button" Text="Cancel" CssClass="button_gray_m">
            <EventHandler Name="cancel_onclick" Event="onclick" Function="SwitchForm(ser.srv.dialplan.form.DialplanListForm)"  ShortcutKey="Escape" ContextMenu="Cancel"/>
        </Element>
    </ActionPanel> 
    <NavPanel>
    </NavPanel> 
    <SearchPanel>
    </SearchPanel>
</EasyForm>
