<EasyForm Name="MtreesCopyForm" Class="EasyForm" FormType="Edit" jsClass="jbForm" Title="Copy Mtrees" Description="" BizDataObj="ser.srv.mtree.do.MtreesDO" TemplateEngine="Smarty" TemplateFile="detail.tpl" EventName="" MessageFile="">
